package com.example.arithmetic.array;

public class ArithmeticArray {
	
	public void arithmeticOperation(int [] intArray) {
		
		try {
			for(int i=0;i<intArray.length;i++) {
				System.out.println(intArray[i]);
			}
		}catch(Exception e) {
			e.printStackTrace();
		}
	}

	public static void main(String[] args) {

		int [] intArray = {1,2,3,4,5,6};
		
		ArithmeticArray arithmeticArray = new ArithmeticArray();
		arithmeticArray.arithmeticOperation(intArray);
	}
}
